				.setOcSupportedFeatures(newOcSupportedFeaturesForTest())
				.setUserIdentifier(UserIdentifier.newBuilder()
						.setExternalIdentifier(&quot;eId_Value&quot;)
						.setLmsi(copyFrom(&quot;Lmsi_Value&quot;))
