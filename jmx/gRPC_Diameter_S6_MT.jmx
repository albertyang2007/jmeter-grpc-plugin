	public Message buildRequest() {
		CirS6tRequest request = CirS6tRequest.newBuilder()
				.setBaseCommand(baseCommand)
				.setBusinessContext(businessCtx)
				.addMonitoringEventConfiguration(monEventConf)
				.addAeseCommunicationPattern(AeseCommunicationPattern.newBuilder()
						.setScefId(copyFrom(&quot;scefId_Value&quot;))
						.setScefReferenceId(Unsigned32.newBuilder().setValue(2000).build())
						.build())
				.setCirFlags(Unsigned32.newBuilder().setValue(1).build())
				.setEnhancedCoverageRestriction(EnhancedCoverageRestriction.newBuilder()
